    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "~8.1.0",
    "bootstrap": "^4.0.0",
    "connect-livereload": "~0.6.0",
    "cssnano": "~3.10.0",
    "dotenv": "^4.0.0",
    "grunt": "~1.0.2",
    "grunt-bg-shell": "~2.3.1",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-postcss": "~0.9.0",
    "grunt-sass": "~2.1.0",
    "jquery": "^3.2.1-slim",
    "load-grunt-tasks": "~3.2.0",
    "pixrem": "~4.0.1",
    "popper.js": "^1.12.3",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "time-grunt": "~1.2.1",
    "webpack-bundle-tracker": "^0.2.1"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "dev": "grunt serve",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "frontend/src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/frontend/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/frontend/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/frontend/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/frontend/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/frontend/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
